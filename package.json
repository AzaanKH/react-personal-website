  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "dev:full": "node dev-with-tests.js",
    "dev:proxy": "concurrently \"npm run dev\" \"sleep 3 && node dev-proxy.js\"",
    "build": "npm run test:steam && vite build",
    "build:skip-tests": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:steam": "node tests/steam-api/steam-api.test.js",
    "test:steam:dev": "TEST_BASE_URL=http://localhost:8888 node tests/steam-api/steam-api.test.js",
    "test:steam:run": "node tests/steam-api/run-tests.js",
    "pretest:steam": "echo 'Testing Steam API integration...'",
    "prebuild": "echo 'Running pre-build Steam API tests...'"
